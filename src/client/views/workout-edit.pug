div(id="page-wrapper" ng-class="{'open': toggle}" ng-controller="DashboardController" ng-cloak)
	//sidebar
	include ./partials/sidebar.pug
	//dashboard header
	include ./partials/dashboard-header.pug
	//main content
	//user management list
	div(class="row")
		div(class="col-lg-12")
			div
				h2 {{ workout.first_name }} {{ workout.last_name }}
			main(ng-repeat="item in collection")
				div
					h3 {{ item.title }}
					p {{ item.description }}
					img(ng-src="{{ item.image  }}")
					p {{ item.def_rep_start }}
					p {{ item.def_rep_end }}
					p {{ item.def_set_start }}
					p {{ item.def_set_end }}